<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.halohoop.androiddigin">

    <application
        android:name=".managers.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:icon="@mipmap/ic_launcher_round">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".showacts.ColorMatrixActivity" />
        <activity android:name=".showacts.RevealActivity" />
        <activity android:name=".showacts.RadialGradientActivity" />
        <activity android:name=".showacts.SweepGradientActivity" />
        <activity
            android:name=".showacts.MenuUsageActivity"
            android:label="@string/title_activity_menu_usage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".NavMainActivity"
            android:label="@string/title_activity_nav_main" />
        <activity android:name=".showacts.ShowActivity" />
        <activity android:name=".showacts.TmpShowActivity" />
        <activity
            android:name=".materialdesign.MDMainActivity"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/material_design"
            android:theme="@style/MDAppTheme"></activity>
        <activity
            android:name=".showacts.FragmentPopStackActivity"
            android:label="@string/title_activity_fragment_popstack_usage"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>