<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.halohoop.androiddigin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MagnifierActivity" />
        <activity android:name=".ColorMatrixActivity" />
        <activity android:name=".RevealActivity" />
        <activity android:name=".RadialGradientActivity" />
        <activity android:name=".SweepGradientActivity" />
        <activity android:name=".ScratchActivity" />
        <activity
            android:name=".MenuUsageActivity"
            android:label="@string/title_activity_menu_usage"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>